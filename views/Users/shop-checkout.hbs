   <script>
    document.getElementById('orderForm').addEventListener('submit', function (e) {
        e.preventDefault(); // Prevent the form from submitting traditionally

        // Gather form data
        const formData = new FormData(this);
        const formObject = Object.fromEntries(formData.entries());

        // Send the data via AJAX
        fetch('/Place-Order', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
                'X-Requested-With': 'XMLHttpRequest'
            },
            body: JSON.stringify(formObject)
        })
        .then(response => response.json())
        .then(data => {
            if (data.success) {
                if (data.paymentMethod === 'Razorpay') {
                    // Initialize Razorpay if Razorpay was selected
                    const options = {
                        "key": data.razorpayKey, // Enter the Key ID generated from the Dashboard
                        "amount": data.razorpayOrder.amount, // Amount in paise
                        "currency": "INR",
                        "name": "Evara",
                        "description": "Order Payment",
                        "order_id": data.razorpayOrder._id, // Order ID from Razorpay
                        "handler": function (response) {
                            // After successful payment
                            fetch('/verify-payment', {
                                method: 'POST',
                                headers: {
                                    'Content-Type': 'application/json'
                                },
                                body: JSON.stringify({
                                    razorpay_payment_id: response.razorpay_payment_id,
                                    razorpay_order_id: response.razorpay_order_id,
                                    razorpay_signature: response.razorpay_signature
                                })
                            })
                            .then(verifyResponse => verifyResponse.json())
                            .then(verifyData => {
                                if (verifyData.success) {
                                    alert('Payment successful!');
                                    window.location.href = '/order-confirmation'; // Redirect after success
                                } else {
                                    alert('Payment verification failed.');
                                }
                            })
                            .catch(error => {
                                console.error('Verification error:', error);
                                alert('An error occurred during payment verification.');
                            });
                        },
                        "theme": {
                            "color": "#3399cc"
                        }
                    };
                    const rzp1 = new Razorpay(options);
                    rzp1.open();
                } else if (data.paymentMethod === 'Cash on Delivery') {
                    window.location.href = '/shop-orders'; // Redirect for Cash on Delivery
                }
            } else {
                alert('Error placing order.');
            }
        })
        .catch(error => {
            console.error('Error:', error);
            alert('An error occurred while placing the order.');
        });
    });
</script>

    <script>
    document.getElementById('applyCouponForm').addEventListener('submit', function (e) {
    e.preventDefault(); // Prevent the form from submitting traditionally

    // Gather form data
    const formData = new FormData(this);
    const formObject = Object.fromEntries(formData.entries());

    // Send the data via AJAX
    fetch('/ApplyCouponCode', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
            'X-Requested-With': 'XMLHttpRequest'
        },
        body: JSON.stringify(formObject)
    })
    .then(response => response.json())
    .then(data => {
        if (data.success) {
            // Handle successful coupon application
            alert('Coupon applied successfully!');

            // Update the total price in the UI
            const totalPriceElement = document.querySelector('.product-subtotal span');
            if (totalPriceElement) {
                totalPriceElement.textContent = `$${data.totalPrice.toFixed(2)}`;
            }
        } else {
            alert('Failed to apply coupon: ' + (data.message || 'Unknown error.'));
        }
    })
    .catch(error => console.error('Error:', error));
});
