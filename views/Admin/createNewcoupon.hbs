        <section class="content-main">
            <form action="/Admin/Newcoupon" method="POST">
                <h5 class="card-header">Create Coupon</h5>
                <div class="card-body">
                    <div class="row">
                        <div class="mb-3 col-md-6">
                            <label for="couponCode" class="form-label">Coupon Code:</label>
                            <input class="form-control" type="text" id="couponCode" name="couponCode"
                                placeholder="Enter coupon code" autofocus required />
                        </div>
                        <div class="mb-3 col-md-6">
                            <label class="form-label">Discount Amount (%)</label>
                            <input type="number" class="form-control" name="discountAmount"
                                placeholder="Enter discount amount" required />
                        </div>
                        <div class="mb-3 col-md-6">
                            <label class="form-label">Expiration Date</label>
                            <input type="date" class="form-control" name="expirationDate" required />
                        </div>
                        <div class="mb-3 col-md-6">
                            <label class="form-label">Minimum Purchase Amount</label>
                            <input type="number" class="form-control" name="minPurchaseAmount"
                                placeholder="Minimum purchase amount" />
                        </div>
                    </div>
                    <div class="mt-2">
                        <button type="submit" class="btn btn-primary me-2">Create Coupon</button>
                    </div>
                </div>
            </form>
        </section> <!-- content-main end// -->
